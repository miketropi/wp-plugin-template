{
	"compilerOptions": {
		"target": "ES2017",
		"module": "ESNext",
		"jsx": "react-jsx",
		"strict": false,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"typeRoots": ["./node_modules/@types", "./types"],
		"types": []
	},
	"include": ["assets/**/*"]
}
